{
    "name": "obsidian-jira-tracker",
    "version": "1.0.0-BETA",
    "description": "This plugin lets you to track [Atlassian Jira](https://www.atlassian.com/software/jira) tickets in [Obsidian](https://obsidian.md/).",
    "main": "src/main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs dev",
        "build": "tsc -noEmit && node esbuild.config.mjs",
        "lint": "eslint .",
        "version": "node version-bump.mjs",
        "test": "cross-env-shell TZ=UTC jest",
        "test-watch": "cross-env-shell TZ=UTC \"jest --watchAll\""
    },
    "keywords": [
        "obsidian",
        "Jira",
        "Atlassian"
    ],
    "author": "rchl-pttrsn",
    "license": "AGPL-3.0-or-later",
    "devDependencies": {
        "@eslint/js": "^9.25.1",
        "@types/jest": "^29.5.14",
        "@types/jsonpath": "^0.2.4",
        "@types/ms": "^0.7.34",
        "@types/node": "^18.19.42",
        "builtin-modules": "^3.3.0",
        "cross-env": "^7.0.3",
        "esbuild": "^0.25.3",
        "eslint": "^8.57.1",
        "jest": "^29.7.0",
        "jsonpath": "^1.1.1",
        "obsidian": "latest",
        "openapi-typescript": "^7.6.1",
        "ts-jest": "^29.3.2",
        "tslib": "2.4.1",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.31.1"
    },
    "dependencies": {
        "@codemirror/language": "^6.10.2",
        "@codemirror/state": "^6.4.1",
        "@codemirror/view": "^6.29.0",
        "@lezer/common": "^1.2.1",
        "colorsys": "^1.0.22",
        "escape-string-regexp": "^5.0.0",
        "moment": "^2.30.1",
        "ms": "^2.1.3",
        "ts-node": "^10.9.2"
    }
}
